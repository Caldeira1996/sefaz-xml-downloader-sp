{
  "name": "sefaz-proxy-server",
  "version": "1.0.0",
  "description": "Servidor proxy para comunicação com SEFAZ SP",
  "main": "server.js",
  "scripts": {
    "start": "node server-https.js",
    "start:https": "node server-https.js",
    "start:http": "node server.js",
    "dev": "nodemon server.js",
    "dev:https": "nodemon server-https.js",
    "setup-ssl": "chmod +x generate-ssl.sh && ./generate-ssl.sh",
    "test-ssl": "curl -k https://localhost:3002/health"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-xml-parser": "^5.2.5",
    "forge": "^2.3.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "pg": "^8.16.3",
    "soap": "^0.45.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
